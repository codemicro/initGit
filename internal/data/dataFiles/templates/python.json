    },

    "commands": [
        {
            "command": "poetry init",
            "stdin": [
                "{projectName}",
                "",
                "",
                "",
                "",
                "{spdxLicense}",
                "",
                "no",
                "no",
                "yes"
            ]
        }
    ]
}